{
  "name": "click2pass client",
  "version": "0.0.1",
  "manifest_version": 2,

  "options_page": "options.html",
  "permissions": [
    "storage"
  ],

  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["md5.js", "mt.js", "main.js"],
      "run_at": "document_end"
    }
  ]
}
